{
  "name": "implementation",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@truffle/contract": "^4.0.33",
    "any-promise": "^1.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "erc721x": "https://github.com/loomnetwork/erc721x",
    "eth-gas-reporter": "^0.1.10",
    "ethlint": "^1.2.3",
    "ganache-cli": "^6.4.1",
    "keythereum": "^1.0.4",
    "openzeppelin-solidity": "^2.2.0",
    "rlp": "^2.0.0",
    "truffle": "^5.0.9",
    "truffle-hdwallet-provider": "^1.0.0-web3one.0",
    "truffle-privatekey-provider": "0.0.5",
    "web3-utils": "^1.0.0-beta.34",
    "websocket": "^1.0.28"
  },
  "devDependencies": {
    "bip39": "^2.5.0",
    "openzeppelin-test-helpers": "^0.3.0"
  },
  "scripts": {
    "console": "./node_modules/.bin/truffle console --network local_ganache",
    "compile": "./node_modules/.bin/truffle compile",
    "test": "./node_modules/.bin/truffle test",
    "test:gas": "GAS_REPORTER=true yarn test",
    "lint": "./node_modules/.bin/solium --dir ./contracts",
    "ganache-cli:dev": "scripts/ganache-cli.sh",
    "migrate:dev": "rm -rf build && yarn ganache-cli:dev && yarn migrate:dev:contracts",
    "migrate:dev:contracts": "./node_modules/.bin/truffle migrate --all --network local_ganache",
    "migrate:erc20:dev": "rm -rf build && yarn ganache-cli:dev && yarn migrate:dev:erc20:contracts",
    "migrate:dev:erc20:contracts": "yarn migrate --all --f 2 --to 3 --network local_ganache",
    "migrate:prod": "./node_modules/.bin/truffle migrate --all --network prod",
    "migrate:erc20:prod": "./node_modules/.bin/truffle migrate -f 2 -- to 3 --network prod",
    "migrate:rinkeby": "rm -rf build && ./node_modules/.bin/truffle migrate --all --network rinkeby",
    "migrate:erc20:rinkeby": "rm -rf build && ./node_modules/.bin/truffle migrate --f 2 --to 3 --network rinkeby",
    "migrate:mainnet": "rm -rf build && ./node_modules/.bin/truffle migrate --f 2 --network mainnet",
    "migrate:mainnet:full": "rm -rf build && ./node_modules/.bin/truffle migrate --all --network mainnet",
    "prepublishOnly": "./node_modules/.bin/truffle compile --all",
    "mnemonic": "node scripts/generate_mnemonic.js"
  },
  "author": "",
  "license": "ISC"
}
